    "def search(arr, n, x):\n",
    "    for i in range(0, n):\n",
    "        if(arr[i]==x):\n",
    "            return i\n",
    "    return -1\n",
    "\n",
    "arr = [1,2,3,4,5,6]\n",
    "x = 5\n",
    "n = len(arr)\n",
    "result = search(arr, n, x)\n",
    "print()"
   ]
